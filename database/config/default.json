    "upload-image": {
      "max-size": 5,
      "support": ["image/png", "image/jpeg", "image/jpg", "image/gif"]
    },
    "simple-suma": { "min": 1, "max": 15, "min-input": 1, "max-input": 99 }
  },
  "auth": {
    "update-data": {
      "description": "",
      "social-network-name": "dbproject",
